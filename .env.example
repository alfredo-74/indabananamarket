# Database Configuration
# For local PostgreSQL, use: postgresql://username:password@localhost:5432/database_name
DATABASE_URL=postgresql://postgres:yourpassword@localhost:5432/indabananamarket

# Session Secret (generate a random string)
SESSION_SECRET=change-this-to-a-random-secret-key

# Interactive Brokers Credentials
IBKR_USERNAME=your-ibkr-username
IBKR_PASSWORD=your-ibkr-password

# Safety Auth Key (for production safety endpoints)
SAFETY_AUTH_KEY=your-safety-auth-key

# Node Environment (development or production)
NODE_ENV=development
